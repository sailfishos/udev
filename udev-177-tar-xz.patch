From f617ecf8b619e18e61d77df2b6e56d2fc182ebe7 Mon Sep 17 00:00:00 2001
From: Marko Saukko <marko.saukko@gmail.com>
Date: Thu, 12 Jan 2012 22:00:19 +0200
Subject: [PATCH] tar -J and --xz options are under GPLv3 license. Use xz
 instead for systems not wanting to use gplv3 codes.

Signed-off-by: Marko Saukko <marko.saukko@gmail.com>
---
 Makefile.am |    2 +-
 1 files changed, 1 insertions(+), 1 deletions(-)

diff --git a/Makefile.am b/Makefile.am
index 83e5fa3..c17602d 100644
--- a/Makefile.am
+++ b/Makefile.am
@@ -309,7 +309,7 @@ src_test_udev_CPPFLAGS = $(udev_common_CPPFLAGS)
 
 # packed sysfs test tree
 test/sys/kernel/uevent_seqnum:
-	$(AM_V_GEN)mkdir -p test && tar -C test/ -xJf $(top_srcdir)/test/sys.tar.xz
+	$(AM_V_GEN)mkdir -p test && xz -dc $(top_srcdir)/test/sys.tar.xz | tar -C test/ -x
 
 BUILT_SOURCES += test/sys/kernel/uevent_seqnum
 
